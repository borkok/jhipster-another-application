entity User {
	name String required
}

entity Profile {
	name String required
}

entity HowOften {
	number Integer required, 
	unit Unit required
}

enum Unit {
	DAYS
}

entity Diagnostic {
	name String required,
    description String
}

entity Execution {
	planned LocalDate required,
	done LocalDate,
    doneComment String
}

relationship ManyToOne {
	Execution{diagnostic} to Diagnostic,
    Execution{user} to User
}

relationship ManyToOne {
	HowOften{diagnostic} to Diagnostic,
    HowOften{profile} to Profile
}

relationship ManyToOne {
	User{profile} to Profile
}

